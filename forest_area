south_percent.write(comparechart_south_percent.htmlcontent)
south_percent.close()


#___________________________________________________________Compare Percent multiBarChart(เทียบการลดลง/เพิ่มขึ้นจากปีก่อนหน้า)___________________________________________________

from nvd3 import multiBarHorizontalChart

#data
year = [2519, 2521, 2525, 2528, 2531, 2532, 2534, 2536, 2538, 2541, 2543, 2547, 2548, 2549, 2551, 2556, 2557]
north = [60.32-66.96, 55.96-60.32, 51.73-55.96, 49.59-51.73, 47.39-49.59, 47.29-47.39, 45.47-47.29, 44.35-45.47, 43.55-44.35, 43.06-43.55, 56.75-43.06, 54.27-56.75, 47.31-54.27, 52.09-47.31, 56.04-52.09, 52.36-56.04, 52.60-52.36]
north_east = [24.57-30.01, 18.49-24.57, 15.33-18.49, 15.15-15.33, 14.03-15.15, 13.97-14.03, 12.91-13.97, 12.72-12.91, 12.59-12.72, 12.43-12.59, 15.71-12.43, 16.64-15.71, 15.00-16.64, 14.54-15.00, 16.32-14.54, 15.09-16.32, 15.02-15.09]
east = [34.60-41.19, 30.24-34.60, 21.92-30.24, 21.89-21.92, 21.46-21.89, 21.33-21.46, 21.07-21.33, 20.29-21.07, 20.80-20.29, 20.57-20.80, 23.12-20.57, 22.57-23.12, 21.74-22.57, 21.60-21.74, 21.01-21.60, 22.45-21.01, 22.18-22.45]
central = [32.38-35.56, 30.31-32.38, 27.47-30.31, 26.24-27.47, 25.59-26.24, 25.55-25.59, 24.65-25.55, 24.34-24.65, 24.17-24.34, 23.81-24.17, 31.84-23.81, 31.52-31.84, 30.68-31.52, 30.50-30.68, 29.81-30.50, 32.79-29.81, 32.86-32.79]
south = [28.48-26.07, 24.89-28.48, 23.25-24.89, 21.90-23.25, 20.69-21.90, 20.65-20.19, 19.02-20.65, 18.11-19.02, 17.61-18.11, 17.15-17.61, 24.62-17.15, 25.37-24.62, 24.99-25.37, 24.46-24.99, 27.03-24.46, 23.95-27.03, 23.97-23.95]
extra_serie = {"tooltip": {"y_start": "", "y_end": " Percent"}}

#ภาคเหนือ
multibarchart_north = multiBarHorizontalChart(name='North', height=400, width=800)
multibarchart_north.add_serie(name="North", y=north, x=year, extra=extra_serie)#---ใส่ค่า
multibarchart_north.buildhtml()
northbar = open("multibarchart_north.html", "w")
northbar.write(multibarchart_north.htmlcontent)
northbar.close()

#ภาคตะวันออกเฉียงเหนือ
multibarchart_north_east = multiBarHorizontalChart(name='North_East', height=400, width=800)
multibarchart_north_east.add_serie(name="North_East", y=north_east, x=year, extra=extra_serie)#---ใส่ค่า
multibarchart_north_east.buildhtml()
north_eastbar = open("multibarchart_north_east.html", "w")
north_eastbar.write(multibarchart_north_east.htmlcontent)
north_eastbar.close()

#ภาคตะวันออก
multibarchart_east = multiBarHorizontalChart(name='East', height=400, width=800)
multibarchart_east.add_serie(name="East", y=east, x=year, extra=extra_serie)#---ใส่ค่า
multibarchart_east.buildhtml()
eastbar = open("multibarchart_east.html", "w")
eastbar.write(multibarchart_east.htmlcontent)
eastbar.close()

#ภาคกลาง
multibarchart_central = multiBarHorizontalChart(name='Central', height=400, width=800)
multibarchart_central.add_serie(name="Central", y=central, x=year, extra=extra_serie)#---ใส่ค่า
multibarchart_central.buildhtml()
centralbar = open("multibarchart_central.html", "w")
centralbar.write(multibarchart_central.htmlcontent)
centralbar.close()

#ภาคใต้
multibarchart_south = multiBarHorizontalChart(name='South', height=400, width=800)
multibarchart_south.add_serie(name="South", y=south, x=year, extra=extra_serie)#---ใส่ค่า
