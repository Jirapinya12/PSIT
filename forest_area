#__________________________________________________________________Map Thailand(แผนที่ประเทศไทย)_____________________________________________________________________#

import folium
from PIL import Image

map_thailand = folium.Map(location=[15.0000, 100.0000], width=1920, height = 800, zoom_start=5) # [ละติจูด,ลองติดจูด] และการซูมลำดับ 5
pic_north = "<img src='http://image.free.in.th/v/2013/ii/151205072558.gif'  height='300' width='300'>"
pic_north_east = "<img src='http://i.imgur.com/QqZGLug.gif'  height='300' width='300'>"
pic_east = "<img src='http://www.mx7.com/i/c60/MGlkic.gif'  height='300' width='300'>"
pic_south = "<img src='http://image.free.in.th/v/2013/io/151205080659.gif'  height='300' width='300'>"
